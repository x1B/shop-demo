{
   "name": "shop-demo",
   "description": "A demo application intended to make learning LaxarJS simple.",
   "version": "2.0.0-pre",
   "private": true,
   "repository": {
      "type": "git",
      "url": "https://github.com/LaxarJS/shop-demo.git"
   },
   "license": "MIT",
   "scripts": {
      "browser-spec": "npm run browser-spec-build && npm run browser-spec-run",
      "browser-spec-build": "webpack --env.browserSpec",
      "browser-spec-run": "webpack-dev-server --env.browserSpec",
      "eslint": "eslint --ext=.js,.vue application/widgets nightwatch-tests/{conf,lib,tests} *.js",
      "karma": "node karma.config.js",
      "dist": "webpack -p --env.production",
      "prepublish": "webpack --progress",
      "start": "webpack-dev-server --inline --hot",
      "test": "npm run karma && npm run eslint",
      "yo": "yo"
   },
   "dependencies": {
