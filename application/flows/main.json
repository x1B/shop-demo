{
   "redirectOn": {
      "unknownPlace": "error404"
   },
   "places": {

      "start": {
         "patterns": [ "/" ],
         "redirectTo": "browse",
         "page":  "shop_demo"
      },

      "browse": {
         "patterns": [ "/browse/:search(.*)", "/browse" ],
         "targets": {
            "next": "confirmation"
         },
         "page":  "shop_demo"
      },

      "confirmation": {
         "patterns": [ "/confirm" ],
         "page":  "finish_order"
      },

      "error404": {
         "patterns": [ "/not-found" ],
         "page": "404"
      }

   }
}
